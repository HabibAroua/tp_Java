package Ex4;

public class Robot 
{
	private int x;
	private int y;
	private char orientation;
	
	public void tourterDroit(char o)
	{
		this.orientation=o;
	}
	
	public void avancer()
	{
	     this.x++;
	     this.y++;
	}
	
	public void avancer(int nbPas)
	{
		this.x+=nbPas;
		this.y+=nbPas;
	}
	
	public void afficher()
	
	

}
