package Ex6;

public class Salle 
{
	private String nom;
	private int capacite;
	
	public Salle()
	{
		
	}
	
	public Salle(String nom , int capacite)
	{
		this.nom=nom;
		this.capacite=capacite;
	}
	
	public String getNom()
	{
		return this.nom;
	}
	
	public int getCapacite()
	{
		return this.capacite;
	}

}
