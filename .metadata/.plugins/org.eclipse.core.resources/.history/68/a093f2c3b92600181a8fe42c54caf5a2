package Ex5;

import java.util.ArrayList;

public class Repertoire 
{
	ArrayList list=new ArrayList<Contact>();
	
   public void ajouterContact(Contact c)
   {
	   list.add(c);
   }
   
   public void AjouterAll(Contact[] T)
   {
	   for(int i=0 ; i<T.length ; i++)
	   {
		   list.add(T[i]);
		   
	   }
   }
}
