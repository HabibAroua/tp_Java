package ex2;

public class Personne 
{
	private String nom;
	private String prenom;
	private String etatCivil;
    private Mere mere;
    private Pere pere;
    private Conjoint conjoint;
    
    public Personne(String nom , String prenom , String etatCivil , Mere mere , Pere pere )
    {
    	this.nom=nom;
    	this.prenom=prenom;
    	this.etatCivil=etatCivil;
    	this.mere=mere;
    	this.pere=pere;
    }


}
