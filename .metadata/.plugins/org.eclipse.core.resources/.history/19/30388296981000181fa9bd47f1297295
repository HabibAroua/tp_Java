package p2;

public class Point 
{
	private int x;
	private int y;
	
	public int getX()
	{
		return this.x;
	}
	
	public int getY()
	{
		return this.y;
	}
	
	public void setX(int x)
	{
		this.x=x;
	}
    
	public void setY(int y)
	{
		this.y=y;
	}
	
	public Point()
	{
		
	}
	
	public Point(int x , int y)
	{
		this.x=x;
		this.y=y;
	}
	
	public void affichage()
	{
		System.out.println("L'abssice est "+this.x+" l'ordonn√© est "+this.y);
	}


	
}
